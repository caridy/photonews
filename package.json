    "yui": "~3.13.0"
  },
  "devDependencies": {
    "jshint": "~1.1.0",
    "yui-lint": "~0.1.3",
    "grunt": "~0.4.1",
    "grunt-contrib-sass": "~0.5.0"
    "grunt-es6-module-transpiler": "git://github.com/caridy/grunt-es6-module-transpiler",
