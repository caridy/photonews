{
  "name": "photonews",
  "version": "0.0.1",
  "description": "PhotoNews web app showcasing modown components",
  "main": "app.js",
  "dependencies": {
    "debug": "*",
    "express": "3.x",
    "express-annotations": "*",
    "express-map": "*",
    "express-state": "*",
    "express-view": "*",
    "express-yui": "~1.0.0",
    "locator": "*",
    "locator-handlebars": "*",
    "locator-yui": "0.1.x",
    "photonews-lib": "git://github.com/imalberto/photonews-lib.git",
    "grunt-es6-module-transpiler": "git://github.com/caridy/grunt-es6-module-transpiler.git",
    "es6-module-transpiler": "~0.3.4",
    "yui": "3.x"
  },
  "devDependencies": {
    "jshint": "~2.3.0",
    "yui-lint": "~0.1.3",
    "grunt": "~0.4.1",
    "grunt-contrib-sass": "~0.5.0",
    "grunt-contrib-watch": "~0.5.3"
  },
  "scripts": {
    "pretest": "node ./node_modules/.bin/jshint --config ./node_modules/yui-lint/jshint.json ./public/js/**/*.js ./public/js/*.js *.js",
    "xtest": "node ./node_modules/.bin/jshint --config ./node_modules/yui-lint/jshint.json ./public/js/**/*.js ./public/js/*.js *.js"
  },
  "locator": {
    "-rulesets": "app-rulesets.js"
  },
  "repository": "https://github.com/imalberto/photonews",
  "author": "alberto chan <imalberto@gmail.com>",
  "license": "BSD"
}
